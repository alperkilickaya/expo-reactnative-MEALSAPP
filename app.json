{
  "expo": {
    "name": "My Recipe App",
    "slug": "RecipeApp",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#24180f",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#163864"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "2.0.0",
      "supportsTablet": true,
      "bundleIdentifier": "com.alperkilickaya.mealsrecipeapp"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#163864"
      },
      "package": "com.alperkilickaya.mealsrecipeapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
